
  
  {
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "destination": "/index.js"
      }
    ],
     "rewrites": [{ "source": "/(.*)", "destination": "/index.js" }] 
    
  }

  